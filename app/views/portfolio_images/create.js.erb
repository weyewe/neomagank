
<% new_entry =  escape_javascript(render :partial => "portfolios/slider", 
              :locals=> {:images => @images}) %> 
$(document).ready(function(){
  $("#slide2").remove();
  $("#sliderWrapper").prepend('<%= new_entry%>');
  
    $("#slide2").easySlider({
   auto: true, 
   continuous: true,
   speed: 800,
   controlsShow: true,
   numeric: true,
   numericId: 'controls'
  });
    
    
});