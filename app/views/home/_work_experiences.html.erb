<div class="cv-section col-1 fixed">

	<h2 class="cv-section-name">Work Experience</h2>
	<%= render :partial => "work_experience_form" , :locals => {:work_experience => @work_experience}%>
	
  

	
	<ul class="cv-section-items" id="work-experience">
	  <% current_user.work_experiences.order("created_at DESC").each do |work_experience| %>
	    <%= render :partial => "work_experience_entry" , :locals => {:work_experience => work_experience} %>
	  <% end%>
	</ul><!-- end .cv-section-items -->

  <div class="super-actions">
    <a href="#" class="action-button add">add</a>
    <a href="#" class="action-button cancel hidden">cancel</a>
  </div>

</div><!-- end .cv-section -->
